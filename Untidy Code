#include <iostream>
#include <string>
using namespace std;

int main() {
	cout << "This is untidy code. ";
	cout << " I'm surprised it works. ";

	int number = 6;
	cout << " It has " << number << " ines of code - ";
	cout << " each more hideous than the last. ";
	cout << " You probably should add some line breaks in the text too" << endl;

	cin.get(); 
	return 0;
}
